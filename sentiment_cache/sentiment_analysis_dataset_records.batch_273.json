ound_truth": null}, {"id": "759eea30-0d20-4d14-87db-0e3f6fd94941", "text": "\u062a\u0639\u0628\u064a\u062f \u0627\u0644\u0637\u0631\u0642 \u0648 \u0625\u0635\u0644\u0627\u062d \u0627\u0644\u062d\u0641\u0631 \u0628\u0634\u0648\u0627\u0631\u0639 \u0648 \u0623\u0632\u0642\u0629 \u0627\u0644\u0645\u062f\u064a\u0646\u0629.", "ground_truth": null}, {"id": "75a2cabe-514d-4d70-93c4-091bb7f889b5", "text": "\u0648\u064a\u0645\u0643\u0631\u0648\u0646 \u0648\u064a\u0645\u0643\u0631 \u0627\u0644\u0644\u0647 \u0648\u0627\u0644\u0644\u0647 \u062e\u064a\u0631 \u0627\u0644\u0645\u0627\u0643\u0631\u064a\u0646 \u0635\u062f\u0642 \u0627\u0644\u0644\u0647 \u0627\u0644\u0639\u0638\u064a\u0645", "ground_truth": null}, {"id": "75a55c28-e471-42a2-8e10-fa81fbd756b1", "text": "\u0645\u0634\u0631\u0648\u0639 \u0644\u062a\u062d\u0633\u064a\u0646 \u062a\u0639\u0644\u064a\u0645 \u0641\u064a \u0645\u0646\u0637\u0642\u0629 \u0639\u064a\u0646 \u0639\u0631\u0645\u0629", "ground_truth": null}, {"id": "75a5f3df-e8fb-43a5-b595-c172215306c9", "text": "\u062a\u062d\u062f\u064a\u062b \u0639\u0627\u0645 \u062d\u0648\u0644 \u0633\u064a\u0631 \u0627\u0644\u0645\u0634\u0631\u0648\u0639", "ground_truth": null}, {"id": "75a82fd1-ae1d-4105-9b22-4f2d522c6ccc", "text": "\u062a\u0641\u0627\u0635\u064a\u0644 \u0625\u0636\u0627\u0641\u064a\u0629 \u062d\u0648\u0644 \u0634\u0648\u0627\u0631\u0639 \u0648 \u062a\u0637\u0648\u064a\u0631 \u0627\u0628\u062a\u0643\u0627\u0631 \u0645\u0646 \u062e\u0644\u0627\u0644 \u0627\u0644\u0648\u0632\u0627\u0631\u0629 \u0627\u0644\u0648\u0635\u064a\u0629 \u0627\u0644\u0645\u062d\u0644\u064a\u0629", "ground_truth": "positive"}, {"id": "75aaa280-a3ab-416e-902b-76612ef0cb9c", "text": "\u062a\u0639\u062f\u064a\u0644 \u0627\u0644\u0645\u064a\u0632\u0627\u0646\u064a\u0629: \u0632\u064a\u0627\u062f\u0629 \u0628\u0646\u0633\u0628\u0629 19%", "ground_truth": null}, {"id": "75ab4990-16cd-43cd-8fce-551c423b616d", "text": "\u0645\u0634\u0631\u0648\u0639 \u0644\u062a\u062d\u0633\u064a\u0646 \u0628\u06