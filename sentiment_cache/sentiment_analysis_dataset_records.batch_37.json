[{"id": "1076fefa-bf2c-4230-bb63-5d30b99f723b", "text": "26216287-baf6-4465-bbae-0ff647d0aa7a", "ground_truth": "neutral"}, {"id": "10770ea9-086e-4cd5-b02f-8ccfda743389", "text": "\u0645\u0634\u0631\u0648\u0639 \u0644\u062a\u062d\u0633\u064a\u0646 \u0631\u064a\u0627\u0636\u0629 \u0641\u064a \u0645\u0646\u0637\u0642\u0629 \u0627\u0644\u0628\u0647\u0627\u0644\u064a\u0644", "ground_truth": null}, {"id": "10782c1f-e94a-4bb1-b59d-4297e2937dfd", "text": "\u062a\u062d\u062f\u064a\u062b \u0639\u0627\u0645 \u062d\u0648\u0644 \u0633\u064a\u0631 \u0627\u0644\u0645\u0634\u0631\u0648\u0639", "ground_truth": null}, {"id": "10796771-ae04-40c8-a978-ad35056a469b", "text": "757e2607-3952-4ca8-8f03-bffc7c69b50f", "ground_truth": "neutral"}, {"id": "107b1c03-cd11-4a70-94dc-1a19347f64c6", "text": "\u062a\u0641\u0627\u0635\u064a\u0644 \u0625\u0636\u0627\u0641\u064a\u0629 \u062d\u0648\u0644 \u0623\u0645\u0627\u0646 \u0648 \u062a\u0637\u0648\u064a\u0631 \u0628\u064a\u0626\u0629 \u0645\u0646 \u062e\u0644\u0627\u0644 \u0627\u0644\u0645\u0648\u0627\u0631\u062f \u0627\u0644\u0628\u0634\u0631\u064a\u0629 \u0627\u0644\u0645\u062d\u0644\u064a\u0629", "ground_truth": "negative"}, {"id": "107c00ca-b80f-49c1-88cf-ef5a9c62c101", "text": "\u062a\u0623\u062e\u064a\u0631 \u0641\u064a \u0627\u0644\u062c\u062f\u0648\u0644 \u0627\u0644\u0632\u0645\u0646\u064a: 3 \u0623\u0634\u0647\u0631", "ground_truth": null}, {"id": "108493c0-347d-40ad-9e1c-e93475b2de93", "text": "\u062a\u0641\u0627\u0635\u064a\u0644 \u0625\u0636\u0627\u0641\u064a\u0629 \u062d\u0648\u0644 \u0627\u0633\u062a\u062f\u0627\u0645\u0629 \u0648 \u0627\u0642\u062a\u0631\u0627\u062d \u0628\u062a\u0641\u0639\u064a\u0644 \u0634\u0641\u0627\u0641\u064a\u0629 \u0628\u0645\u064a\u0632\u0627\u0646\u064a\u0629 \u062a\u0642\u062f\u064a\u0631\u064a\u0629 \u0627\u0644\u0645\u0648\u0627\u0631\u062f \u0627\u0644\u0628\u0634\u0631\u064a\u0629", "ground_truth": "positive"}, {"id": "1084c879-bf71-4778-b72e-687958d0f4f3", "text": "5d4f2170-35e7-4cfd-953d-9d928a3fbad4", "ground_truth": "negative"}, {"id": "10896bcc-9730-443c-9eca-1b0ec5ba5b78", "text": "2e281d55-92e4-4b50-9199-994765db296d", "ground_truth": "positive"}, {"id": "109008fa-afee-4e29-95d8-90ad5fa279bc", "text": "\u0627\u0644\u062d\u0643\u0648\u0645\u0627\u062a \u0641\u064a \u062c\u0645\u064a\u0639 \u062f\u0648\u0644 \u0627\u0644\u0639\u0627\u0644\u0645\u060c \u0625\u0630\u0627 \u0631\u0623\u062a \u0623\u0646 \u0627\u0644\u0646\u0642\u0627\u0628\u0627\u062a \u062a\u0642\u0648\u0645 \u0628\u062f\u0648\u0631\u0647\u0627 \u0628\u0634\u0643\u0644 \u062f\u064a\u0645\u0642\u0631\u0627\u0637\u064a \u0648\u062a\u062a\u0645\u0633\u0643 \u0628\u0627\u0644\u062d\u064a\u0627\u062f\u060c \u0648\u062a\u062a\u062f\u062e\u0644 \u0641\u0639\u0644\u0627\u064b \u0644\u062d\u0644 \u0627\u0644\u0645\u0634\u0627\u0643\u0644 \u0628\u0634\u0643\u0644 \u064a\u062e\u062f\u0645 \u062c\u0645\u064a\u0639 \u0627\u0644\u0623\u0637\u0631\u0627\u0641 \u062f\u0648\u0646 \u0627\u0644\u0627\u0646\u062d\u064a\u0627\u0632 \u0625\u0644\u0649 \u062c\u0647\u0629 \u0645\u0639\u064a\u0646\u0629 \u0641\u0645\u0646 \u0627\u0644\u0645\u0624\u0643\u062f \u0623\u0646\u0647\u0627 \u0633\u062a\u062a\u0639\u0627\u0648\u0646 \u0645\u0639\u0647\u0627 \u0648\u062a\u062d\u0627\u0648\u0631\u0647\u0627. \u0623\u0645\u0627 \u0625\u0630\u0627 \u0643\u0627\u0646\u062a \u0627\u0644\u062d\u0643\u0648\u0645\u0627\u062a \u062a\u0639\u0644\u0645 \u0623\u0646 \u0627\u0644\u0646\u0642\u0627\u0628\u0627\u062a \u0644\u0627 \u062a\u0645\u0644\u0643 \u0631\u0624\u064a\u0629 \u0648\u0627\u0636\u062d\u0629 \u0623\u0648 \u0645\u0642\u062a\u0631\u062d\u0627\u062a \u062d\u0642\u064a\u0642\u064a\u0629 \u0644\u0645\u0639\u0627\u0644\u062c\u0629 \u0645\u0634\u0627\u0643\u0644 \u0627\u0644\u0645\u0648\u0638\u0641\u064a\u0646 \u0648\u0627\u0644\u0639\u0645\u0627\u0644 \u0628\u0644 \u062a\u0639\u062a\u0645\u062f \u0633\u064a\u0627\u0633\u0629 \u0627\u0644\u062a\u062d\u0631\u064a\u0636 \u0648\u0627\u0644\u062a\u0623\u062c\u064a\u062c \u0641\u0644\u0646 \u062a\u0633\u062a\u0639\u064a\u0646 \u0628\u0647\u0627 \u0623\u0648 \u062a\u0644\u062c\u0623 \u0625\u0644\u064a\u0647\u0627.\u0625\u0635\u062f\u0627\u0631 \u0642\u0627\u0646\u0648\u0646 \u064a\u0646\u0638\u0645 \u0627\u0644\u0625\u0636\u0631\u0627\u0628 \u0623\u062d\u064a\u0627\u0646\u064b\u0627 \u064a\u0643\u0648\u0646 \u0646\u062a\u064a\u062c\u0629 \u0644\u063a\u064a\u0627\u0628 \u0631\u0624\u064a\u0629 \u0646\u0642\u0627\u0628\u064a\u0629 \u0648\u0627\u0636\u062d\u0629. \u0648\u0647\u0646\u0627\u0643 \u0646\u0642\u0627\u0628\u0627\u062a \u0644\u0645 \u062a\u062a\u0645\u0643\u0646 \u0645\u0646 \u062d\u0645\u0627\u064a\u0629 \u0627\u0644\u0639\u0645\u0627\u0644 \u0627\u0644\u0630\u064a\u0646 \u0641\u0642\u062f\u0648\u0627 \u0648\u0638\u0627\u0626\u0641\u0647\u0645 \u0628\u0633\u0628\u0628 \u0627\u0646\u062a\u0645\u0627\u0626\u0647\u0645 \u0627\u0644\u0646\u0642\u0627\u0628\u064a. \u0641\u064a \u0627\u0644\u0645\u0642\u0627\u0628\u0644 \u0638\u0647\u0631\u062a \u062a\u0646\u0633\u064a\u0642\u064a\u0627\u062a \u062e\u0644\u0627\u0644 \u0633\u0646\u0648\u0627\u062a \u0642\u0644\u064a\u0644\u0629 \u062a\u0645\u0643\u0646\u062a \u0645\u0646 \u062a\u062d\u0642\u064a\u0642 \u0645\u0643\u0627\u0633\u0628 \u0644\u0645 \u062a\u0633\u062a\u0637\u0639 \u0627\u0644\u0646\u0642\u0627\u0628\u0627\u062a \u062a\u062d\u0642\u064a\u0642\u0647\u0627. \u0643\u0645\u0627 \u0623\u0646 \u0638\u0647\u0648\u0631 \u0628\u0639\u0636 \u0627\u0644\u0633\u0644\u0648\u0643\u064a\u0627\u062a \u063a\u064a\u0631 \u0627\u0644\u0623\u062e\u0644\u0627\u0642\u064a\u0629 \u0645\u0646 \u0628\u0639\u0636 \u0627\u0644\u0645\u0646\u062a\u0633\u0628\u064a\u0646 \u0644\u0644\u0646\u0642\u0627\u0628\u0627\u062a\u060c \u0627\u0644\u062a\u064a \u064a\u0645\u0643\u0646 \u0627\u0639\u062a\u0628\u0627\u0631\u0647\u0627 \u0634\u0643\u0644\u0627\u064b \u0645\u0646 \u0623\u0634\u0643\u0627\u0644 \u0627\u0644\u0628\u0644\u0637\u062c\u0629 \u0623\u0633\u0647\u0645 \u0641\u064a \u0632\u064a\u0627\u062f\u0629 \u0641\u0642\u062f\u0627\u0646 \u0627\u0644\u062b\u0642\u0629 \u0641\u064a \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u0646\u0642\u0627\u0628\u064a.", "ground_truth": null}, {"id": "1092f465-382a-4b0b-966d-bfd626048563", "text": "\u062a\u0641\u0627\u0635\u064a\u0644 \u0625\u0636\u0627\u0641\u064a\u0629 \u062d\u0648\u0644 \u0646\u0633\u0627\u0621 \u0648 \u062a\u0637\u0628\u064a\u0642 \u0646\u0633\u0627\u0621 \u0643\u062d\u0644 \u0645\u0633\u062a\u062f\u0627\u0645 \u0628\u0627\u0633\u062a\u062e\u062f\u0627\u0645 12", "ground_truth": "negative"}, {"id": "10988e67-e2d6-4915-acbe-164f510cf9be", "text": "\u0627\u0644\u0645\u063a\u0631\u0628 \u0628\u0644\u062f \u0639\u0644\u0645\u0627\u0646\u064a. \u0646\u062d\u0646 \u062f\u0648\u0644\u0629 \u0642\u0627\u0646\u0648\u0646\u060c \u0645\u062b\u0644 \u0643\u0644 \u0627\u0644\u062f\u0648\u0644 \u0627\u0644\u062f\u064a\u0645\u0642\u0631\u0627\u0637\u064a\u0629. \u0623\u0645\u0627 \u0645\u0646 \u064a\u0631\u064a\u062f \u0627\u0644\u062f\u0648\u0644\u0629 \u0627\u0644\u0625\u0633\u0644\u0627\u0645\u064a\u0629 \u0641\u0639\u0644\u064a\u0647 \u0623\u0646 \u064a\u0630\u0647\u0628 \u0625\u0644\u0649 \u0623\u0641\u063a\u0627\u0646\u0633\u062a\u0627\u0646 \u062b\u0645 \u064a\u0639\u064a\u0634. \u0648\u0623\u0646\u0627 \u0623\u062a\u0641\u0642 \u0645\u0639 \u0645\u0627 \u0642\u0627\u0644\u0647 \u0627\u0644\u0648\u0632\u064a\u0631. \u0648\u0641\u064a \u0636\u0648\u0621 \u0623\u0639\u0645\u0627\u0644 \u0627\u0644\u062f\u064a\u0646\u060c \u0641\u0625\u0646\u0646\u0627 \u0646\u0639\u064a\u0634 \u0641\u064a \u0639\u0627\u0645 2024. \u0648\u0627\u0644\u062e\u064a\u0627\u0631 \u0647\u0648 \u0623\u0646 \u0645\u0646 \u0623\u0631\u0627\u062f \u0623\u0646 \u064a\u0624\u0645\u0646 \u0639\u0644\u064a\u0647 \u0623\u0646 \u064a\u0624\u0645\u0646\u060c \u0648\u0645\u0646 \u0623\u0631\u0627\u062f \u063a\u064a\u0631 \u0630\u0644\u0643 \u0641\u0644\u064a\u0641\u0639\u0644. \u0627\u0644\u0628\u0644\u062f \u0628\u062d\u0627\u062c\u0629 \u0625\u0644\u0649 \u06