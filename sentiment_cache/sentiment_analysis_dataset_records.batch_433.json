[{"id": "ba021c6a-1d93-40aa-ab3c-bc7fbb57c0c3", "text": "3 \u0645\u0644\u064a\u0627\u0631 \u0644\u0644\u0635\u0627\u0631\u0648\u062e \u0648 \u0643\u064a\u062a\u0641\u0631\u0643\u0639 \u0648 \u0643\u0627 \u064a\u0645\u0634\u064a", "ground_truth": null}, {"id": "ba03b562-42ba-44c5-a111-3c1e408b72f9", "text": "\u0645\u0646 \u0627\u0644\u062c\u064a\u062f \u0623\u0646 \u0646\u0631\u0649 \u0627\u0644\u062d\u0643\u0648\u0645\u0629 \u062a\u0639\u0645\u0644 \u0628\u062a\u0639\u0627\u0648\u0646 \u0648\u062a\u0646\u0627\u063a\u0645 \u0644\u062a\u062d\u0642\u064a\u0642 \u0623\u0647\u062f\u0627\u0641\u0647\u0627\u060c \u0648\u0646\u0623\u0645\u0644 \u0623\u0646 \u064a\u0643\u0648\u0646 \u0644\u0647\u0630\u0627 \u062a\u0623\u062b\u064a\u0631 \u0625\u064a\u062c\u0627\u0628\u064a \u0639\u0644\u0649 \u062d\u064a\u0627\u0629 \u0627\u0644\u0645\u0648\u0627\u0637\u0646\u064a\u0646", "ground_truth": null}, {"id": "ba04f9c0-f895-4f2b-b6aa-aea9437f765f", "text": "\u0627\u0644\u0645\u062c\u0627\u0644 \u0627\u0644\u0631\u064a\u0627\u0636\u064a : \u0636\u0631\u0648\u0631\u0629 \u0627\u062d\u062f\u0627\u062b \u0645\u0644\u0627\u0639\u0628 \u0627\u0644\u0642\u0631\u0628 \u2013 \u062a\u0623\u0647\u064a\u0644 \u0627\u0644\u0645\u0644\u0639\u0628 \u0627\u0644\u062c\u0645\u0627\u0639\u064a \u0648 \u0627\u0646\u0634\u0627\u0621 \u0641\u0636\u0627\u0621\u0627\u062a \u0627\u0644\u0627\u0633\u062a\u0642\u0628\u0627\u0644 \u0648 \u0627\u0646\u0634\u0627\u0621 \u0642\u0631\u064a\u0629 \u0631\u064a\u0627\u0636\u064a\u0629 \u2013 \u0627\u062d\u062f\u0627\u062b \u0645\u0631\u0643\u0628 \u0631\u064a\u0627\u0636\u064a \u0646\u0633\u0627\u0626\u064a", "ground_truth": null}, {"id": "ba069ceb-983f-4298-96de-d4aeabd1fff6", "text": "\u062a\u0641\u0627\u0635\u064a\u0644 \u0625\u0636\u0627\u0641\u064a\u0629 \u062d\u0648\u0644 \u062a\u0643\u0646\u0648\u0644\u0648\u062c\u064a\u0627 \u0648 \u0625\u0637\u0644\u0627\u0642 \u0645\u0628\u0627\u062f\u0631\u0629 \u062a\u0648\u0627\u0635\u0644 \u0644\u062a\u062d\u0633\u064a\u0646 718203 \u062f\u0631\u0647\u0645", "ground_truth": "positive"}, {"id": "ba17798b-b58e-4764-8faf-cc51bf325eae", "text": "\u062a\u063a\u0644\u064a\u0628 \u0627\u0644\u062a\u0648\u0627\u0641\u0642\u0627\u062a \u0627\u0644\u0633\u064a\u0627\u0633\u064a\u0629 \u0639\u0644\u0649 \u0627\u0644\u0625\u0646\u0634\u063a\u0627\u0644\u0627\u062a \u0627\u0644\u062a\u0646\u0645\u0648\u064a\u0629 \u0627\u0644\u0645\u062d\u0644\u064a\u0629", "ground_truth": null}, {"id": "ba185c4f-846a-4561-9e27-06c38728b964", "text": "\u064a\u0642\u0648\u0644 \u0627\u0644\u0645\u062b\u0644 \u0623\u0628\u0639\u062f \u0639\u0646 \u0627\u0644\u0634\u0631 \u0648\u063a\u0646\u064a \u0644\u0648 \u064a\u0643\u0641\u064a\u0646\u0627 \u0645\u0634\u0627\u0643\u0644\u0646\u0627 \u0627\u0644\u062f\u0627\u062e\u0644\u064a\u0629 \u0648\u0645\u0634\u0627\u0643\u0644 \u0627\u0644\u0648\u0636\u0639 \u0627\u0644\u0625\u0642\u062a\u0635\u0627\u062f\u064a \u0648\u0627\u0644\u0645\u0639\u064a\u0634\u0629 \u0627\u0644\u0645\u0631\u0629 \u0627\u0644\u062a\u064a \u064a\u0639\u064a\u0634\u0648\u0646\u0647\u0627 \u0627\u0644\u0645\u0648\u0627\u0637\u0646\u064a\u0646 \u0623\u0645\u0627 \u0623\u0646\u062a \u064a\u0627 \u0627\u0633\u062a\u0627\u0630 \u062e\u0644\u064a\u0643 \u0641\u064a \u0627\u0644\u0645\u062d\u0627\u0643\u0645 ( \u062a\u0647\u0628\u0634 \u062a\u062c\u0628\u062f \u0627\u062d\u0646\u0634 )", "ground_truth": null}, {"id": "ba1c7649-f46d-4af3-a659-87883fd2874c", "text": "\u062a\u0642\u062f\u0645 \u0627\u0644\u0645\u0634\u0631\u0648\u0639 \u0645\u0646 13% \u0625\u0644\u0649 60%", "ground_truth": null}, {"id": "ba1cc82b-0eb8-4c63-b859-6892b2781051", "text": "\u0647\u0644 3045 \u062f\u0631\u0647\u0645 \u0641\u064a \u0627\u0644\u0634\u0647\u0631 \u0633\u064a\u0639\u064a\u0634 \u0628\u0647\u0627 \u0627\u0644\u0645\u0648\u0627\u0637\u0646 \u0627\u0644\u0645\u063a\u0631\u0628\u064a \u0648\u064a\u0646\u0634\u0626 \u0628\u0647\u0627 \u0623\u0633\u0631\u0629\u060c \u0631\u0628\u0645\u0627 \u0641\u064a \u0639\u0647\u0