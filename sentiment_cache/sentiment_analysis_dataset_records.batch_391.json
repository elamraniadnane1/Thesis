[{"id": "a80b6e57-1042-41e2-9e11-69c1825506ba", "text": "4686c135-3ff1-4041-9696-80ea2d5b2abc", "ground_truth": "neutral"}, {"id": "a80bb2d1-2dde-4232-bb95-e178e4f85d4b", "text": "\u0639\u0644\u064a\u0646\u0627 \u0623\u0646 \u0646\u0639\u0627\u0645 \u0627\u0646 \u062d\u0642 \u0627\u0644\u0631\u0623\u064a \u0645\u0643\u0641\u0648\u0644 \u062f\u0633\u062a\u0648\u0631\u064a \u0623\u0643\u0646 \u0644\u0645\u0629 \u0628\u062a\u0639\u0644\u0642 \u0627\u0644\u0627\u0645\u0631 \u0644\u0645\u0645\u062b\u0644 \u0627\u0644\u0623\u0645\u0629 \u0647\u0646\u0627\u0643 \u0628\u062a\u063a\u064a\u0631 \u0627\u0644\u0645\u0648\u0642\u0641. \u064a\u062a\u0648\u062c\u0628 \u0639\u0644\u0628\u0646\u0627 \u062d\u0633\u0646 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u0647\u0630\u0627 \u062d\u0642 \u0627\u0644\u0631\u0627\u064a \u0644\u0623\u0646\u0646\u0627 \u0646\u0645\u062b\u0644 \u0634\u0624\u0648\u0646 \u0627\u0644\u0648\u0637\u0646 \u0648 \u0644\u0627 \u0634\u0624\u0648\u0646 \u0633\u062e\u0635\u064a\u0629", "ground_truth": null}, {"id": "a80bf7da-2e56-4e3e-89d1-c9451a64ce1c", "text": "65cbc5b1-7e41-4ddc-af5b-973d7cd23688", "ground_truth": "neutral"}, {"id": "a80c9f7e-b17b-430e-82f0-f18deb06ae3c", "text": "173f5df1-e376-46e5-bef9-83147dcefbfe", "ground_truth": "positive"}, {"id": "a80f8d3f-265b-471e-a41a-c23218da3660", "text": "\u0641\u064a\u0646 \u0643\u0627\u0646 \u0644\u0642\u062c\u0639 \u0633\u0646\u064a\u0646 \u0647\u0627\u062f\u064a.  \u0639\u0627\u062f \u0628\u0627\u0646\u062a\u0644\u0643\u0645 \u0627\u0644\u064a\u0648\u0645. \u0631\u0627\u0647 \u0647\u0627\u062f \u0634\u0631\u0643\u0629 \u0643\u062a\u0628\u0627\u0646 \u0641 \u062c\u0645\u064a\u0639 \u0627\u0644\u0642\u0646\u0648\u0627\u062a \u0631\u064a\u0627\u0636\u064a\u0629 \u0627\u0644\u0639\u0627\u0644\u0645\u064a\u0629 .  \u0643\u0648\u0637\u064a \u0633\u0628\u0648\u0631 \u062f\u064a\u0627\u0644\u0643\u0645 \u0643\u062a\u0639\u0631\u0641\u0648\u0647\u0627 \u063a\u064a\u0631 \u0646\u062a\u0645\u0627 \u064a\u0645\u0643\u0646 \u062d\u064a\u062a \u0643\u062f\u0641\u0639\u0644\u0643\u0645 \u0641\u0627\u0644\u062e\u0641\u0627\u0621\u0648\u0643\u0644\u0647\u0645 \u062d\u0631\u0627\u0645 \u0646\u0641\u0633  \u0627\u0644\u0645\u0631\u062c\u0639\u064a\u0629 \u0639\u0646\u062f\u0643\u0645  \u0627\u0630\u0627 \u0639\u062a\u062d\u0630\u0631 \u062d\u0630\u0631 \u0645\u0646 \u0627\u0644\u0642\u0645\u0627\u0631 \u0628\u0635\u0641\u0629 \u0639\u0627\u0645\u0629 \u0645\u0627\u0634\u064a \u0645\u0646 \u0634\u0631\u0643\u0629 \u0645\u0639\u064a\u0646\u0629", "ground_truth": null}, {"id": "a80fa2b9-ca81-46a4-91e7-1f5ddbcf967e", "text": "\u062a\u0641\u0627\u0635\u064a\u0644 \u0625\u0636\u0627\u0641\u064a\u0629 \u062d\u0648\u0644 \u0631\u064a\u0627\u0636\u0629 \u0648 \u062a\u0646\u0641\u064a\u0630 \u0635\u062d\u0629 \u0639\u0644\u0649 \u0645\u0631\u0627\u062d\u0644 \u062e\u0644\u0627\u0644 \u0627\u0644\u0642\u0637\u0627\u0639 \u0627\u0644\u062e\u0627\u0635 \u0634\u0647\u0648\u0631", "ground_truth": "negative"}, {"id": "a819cb07-1b6e-491c-8a49-839acba2beaa", "text": "\u0644\u0627\u0646\u0647\u0627 \u0627\u0644\u062c\u0632\u0627\u0621\u0631 \u0627\u0644\u062a\u064a \u0644\u0627\u0632\u0627\u0644\u062a \u062a\u0639\u064a\u0634 \u0641\u064a \u0645\u0627 \u064a\u0634\u0628\u0647 \u0627\u0644\u0643\u0647\u0641 \u0641\u064a \u0627\u0641\u0643\u0627\u0631 \u0627\u0644\u0633\u062a\u064a\u0646\u0627\u062a \u0648\u0627\u0644\u0633\u0628\u0639\u064a\u0646\u0627\u062a \u0645\u0646 \u0627\u0644\u0642\u0631\u0646 \u0627\u0644\u0645\u0627\u0636\u064a .\u062c\u0632\u0627\u0621\u0631 \u0645\u0631\u062a \u0628\u0639\u0634\u0631\u064a\u0629 \u062f\u0645\u0648\u064a\u0629 \u0648\u0627\u0633\u062a\u0641\u0627\u0642\u062a \u0645\u0646 \u0646\u0648\u0645\u0647\u0627  \u0648\u0636\u0646\u062a \u0627\u0646 \u0627\u0644\u0627\u062a\u062d\u0627\u062f \u0627\u0644\u0633\u0648\u0641\u064a\u0627\u062a\u064a \u0648\u062f\u0648\u0644 \u0639\u062f\u0645 \u0627\u0644\u0627\u0646\u062d\u064a\u0627\u0632 \u0648\u0627\u0644\u062d\u0631\u0628 \u0627\u0644\u0628\u0627\u0631\u062f\u0629 \u0645\u0627\u0632\u0627\u0644\u0648\u0627 \u0645\u0648\u062c\u0648\u062f\u064a\u0646.\u0644\u0648 \u0643\u0627\u0646\u062a \u062f\u0648\u0644\u0629 \u0623\u062e\u0631\u0649 \u0628\u0627\u0644\u062c\u0648\u0627\u0631 \u0644\u062a\u063a\u064a\u0631\u062a \u0627\u0644\u0643\u062b\u064a\u0631 \u0645\u0646 \u0627\u0644\u0623\u0645\u0648\u0631 \u0648\u0644\u0643\u0646\u0647\u0627 \u0645\u0642\u0648\u0644\u0629 \u0627\u0644\u0645\u0644\u0643 \u0627\u0644\u062d\u0633\u0646 \u0627\u0644\u062b\u0627\u0646\u064a \u0627\u0644\u062e\u0627\u0644\u062f\u0629 \u0644\u064a\u0639\u0644\u0645 \u0627\u0644\u0639\u0627\u0644\u0645 \u0645\u0639 \u0645\u0646 \u062d\u0634\u0631\u0646\u0627 \u0627\u0644\u0644\u0647 \u0641\u064a \u0627\u0644\u062c\u0648\u0627\u0631", "ground_truth": null}, {"id": "a819fa80-66ec-4c19-a9bf-f5829f5e1225", "text": "\u062a\u0642\u062f\u0645 \u0627\u0644\u0645\u0634\u0631\u0648\u0639 \u0645\u0646 12% \u0625\u0644\u0649 69%", "ground_truth": null}, {"id": "a81c42bd-5050-44d7-8575-ecfdd5624bf5", "text": "\u0645\u0634\u0631\u0648\u0639 \u0644\u062a\u062d\u0633\u064a\u0646 \u062e\u062f\u0645\u0627\u062a \u0641\u064a \u0645\u0646\u0637\u0642\u0629 \u0628\u0627\u0628 \u0645\u0631\u0632\u0648\u0642\u0629", "ground_truth": null}, {"id": "a81ccea1-f3d1-490f-b296-2147bd674161", "text": "\u062a\u0641\u0627\u0635\u064a\u0644 \u0625\u0636\u0627\u0641\u064a\u0629 \u062d\u0648\u0644 \u0646\u0633\u0627\u0621 \u0648 \u062a\u0637\u0648\u064a\u0631 \u062a\u0643\u0646\u0648\u0644\u0648\u062c\u064a\u0627 \u0645\u0646 \u062e\u0644\u0627\u0644 673515 \u062f\u0631\u0647\u0645 \u0627\u0644\u0645\u062d\u0644\u064a\u0629", "ground_truth": "negative"}, {"id": "a8201c90-3198-4eca-8ea5-b4b257c10920", "text": "\u0645\u0634\u0631\u0648\u0639 \u0644\u062a\u062d\u0633\u064a\u0646 \u062a\u0636\u0627\u0645\u0646 \u0641\u064a \u0645\u0646\u0637\u0642\u0629 \u062a\u0627\u0632\u0648\u0637\u0629", "ground_truth": null}, {"id": "a8209080-a2ae-451a-8103-c9d537ee8ec2", "text": "\u0627\u0644\u0633\u0644\u0627\u0645 \u0639\u0644\u064a\u0643\u0645\u060c \u0641\u064a \u0627\u0644\u0639\u0645\u0642 \u0647\u0648 \u0625\u062c\u0631\u0627\u0621 \u062f\u0648\u0646 \u0642\u0635\u062f. \u0644\u0627 \u062a\u0623\u062b\u064a\u0631 \u0639\u0644\u0649 \u0627\u0644\u0637\u0628\u0642\u0629 \u0627\u0644\u0645\u062d\u062a\u0627\u062c\u0629. \u0644\u0646 \u064a\u0633\u062a\u0641\u064a\u062f \u0625\u0644\u0627 \u0630\u0648\u064a \u0627\u0644\u0645\u062f\u0627\u062e\u064a\u0644 \u0627\u0644\u0645\u0631\u062a\u0641\u0639\u0629. \u0627\u0628\u062a\u062f\u0627\u0621 \u0645\u0646 20000\u062f\u0631\u0647\u0645. \u0644\u0623\u0646 \u0631\u0628\u062d \u0623\u0642\u0644 \u0645\u0646 40 \u062f\u0631\u0647\u0645 \u0644\u0645\u062f\u062e\u0648\u0644 8000\u062f\u0631\u0647\u0645 \u0644\u0646 \u064a\u062d\u0633\u0646 \u0645\u0633\u062a\u0648\u0649 \u0627\u0644\u0639\u064a\u0634. \u0648 \u0623\u0642\u0644 \u0645\u0646 8000 \u062f\u0631\u0647\u0645 \u0644\u0627 \u064a\u0639\u0646\u064a\u0647 \u0627\u0644\u0642\u0631\u0627\u0631. \u0630\u0648\u064a \u0627\u0644\u0645\u062f\u0627\u062e\u064a\u0644 \u0623\u0642\u0644 \u0644\u0627 \u0645\u0643\u0627\u0646 \u0644\u0647\u0645\u2026. \u064a\u0628\u064a\u0639\u0648\u0646 \u0627\u0644\u0639\u0646\u0627\u0648\u064a\u0646. \u0639\u0644\u0627\u0648\u0629 \u0639\u0644\u0649 \u0623\u0646 \u0645\u062a\u0642\u0627\u0639\u062f\u064a CIMR \u0644\u0627 \u064a\u0639\u0646\u064a\u0647\u0645 \u0627\u0644\u062a\u062e\u0641\u064a\u0636 0,00 \u062f\u0631\u0647\u0645. \u0635\u0641\u0631.", "ground_truth": null}, {"id": "a8221b31-7dd9-4eba-ac1b-e3d72747b2a1", "text": "\u0627\u0644\u062d\u0645\u062f \u0644\u0644\u0647 \u0645\u0644\u0643\u0646\u0627 \u0628\u062e\u064a\u0631 \u0648\u0627\u0644\u0644\u0647 \u064a\u0634\u0641\u064a\u0647 \u0648\u0639\u0627\u0641\u064a\u0647\u0645\u0648\u0631\u064a\u062a\u0627\u0646\u064a\u0627 \u062f\u0648\u0644\u0629 \u0634\u0642\u064a\u0642\u0629 \u0648 \u0627\u0644\u0645\u0648\u0631\u064a\u062a\u0627\u